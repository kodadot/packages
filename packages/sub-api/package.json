{
  "name": "@kodadot1/sub-api",
  "version": "0.1.1-alpha.3",
  "description": "Smart wrapper for Substrate API",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kodadot/packages.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "author": "Viki Val <viktorko99@gmail.com>",
  "type": "module",
