    "c8": "latest",
    "eslint": "latest",
    "express": "^4.18.1",
    "standard-version": "latest",
    "typescript": "latest",
    "unbuild": "latest",
    "vitest": "latest"
  },
  "packageManager": "pnpm@7.0.0"
